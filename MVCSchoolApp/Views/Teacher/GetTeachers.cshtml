@model IEnumerable<MVCSchoolApp.Models.Teacher>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>FirstName</th>
            <th>LastName</th>
            <th>Email</th>
            <th>Phone</th>
            <th>Actions</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var teacher in Model)
        {
            <tr>
                <td>@teacher.FirstName</td>
                <td>@teacher.LastName</td>
                <td>@teacher.Email</td>
                <td>@teacher.Phone</td>
                <td>@Html.ActionLink("Edit", "LoadForm", new { id = teacher.ID })</td>
                <td>@Html.ActionLink("Delete", "Remove", new { id = teacher.ID })</td>
            </tr>
        }
    </tbody>
</table>

